<?xml version="1.0"?>
<configuration>
    <configSections>
        <section name="CassandraSharp" type="CassandraSharp.SectionHandler, cassandra-sharp"/>
    </configSections>

    <CassandraSharp>
        <Cluster name="TestCluster">
            <Behavior maxRetries="3" poolSize="1" defaultKeyspace="unknown" defaultReadConsistencyLevel="ONE" defaultWriteConsistencyLevel="LOCAL_QUORUM"/>

            <Transport type="Framed" port="9160"/>

            <Endpoints strategy="Nearest" snitch="RackInferring">
                <Server>127.0.0.1</Server>
                <Server>127.0.0.2</Server>
            </Endpoints>
        </Cluster>

    </CassandraSharp>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
